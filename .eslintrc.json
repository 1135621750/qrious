{
  "extends": "skelp/v3/es6",
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "max-params": [
      "error",
      5
    ],
    "no-bitwise": "off",
    "no-unused-vars": [
      "warn",
      {
        "args": "none"
      }
    ],
    "prefer-reflect": "off",
    "sort-keys": "off"
  }
}
